<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd"
	default-autowire="byName">


	<bean id="wxAppConfigRepository"
		class="cn.edu.ntu.jtxy.core.repository.wx.impl.WxAppConfigRepositoryImpl" />
	<bean id="refreshTokenRepository"
		class="cn.edu.ntu.jtxy.core.repository.wx.impl.RefreshTokenRepositoryImpl" />
	<bean id="userInfoRepository"
		class="cn.edu.ntu.jtxy.core.repository.wx.impl.UserInfoRepositoryImpl" />
	<bean id="weiXinUserRepository"
		class="cn.edu.ntu.jtxy.core.repository.wx.impl.WeiXinUserRepositoryImpl" />
	<bean id="studentInfoRepository"
		class="cn.edu.ntu.jtxy.core.repository.wx.impl.StudentInfoRepositoryImpl" />
	<bean id="pointRepository"
		class="cn.edu.ntu.jtxy.core.repository.wx.impl.PointRepositoryImpl" />
	<bean id="pointRecordRepository"
		class="cn.edu.ntu.jtxy.core.repository.wx.impl.PointRecordRepositoryImpl" />
</beans>
