package cn.edu.ntu.jtxy.util;

import java.io.UnsupportedEncodingException;
import java.net.URLEncoder;

import cn.edu.ntu.jtxy.web.SystemConstants;

/**
 * 
 * @author {jin.zhang@witontek.com}
 * @version $Id: WebUtils.java, v 0.1 2016年4月27日 上午11:24:03 {jin.zhang@witontek.com} Exp $
 */
public class WebUtils {

    public static String urlEncode(String url) throws UnsupportedEncodingException {
        return URLEncoder.encode(url, SystemConstants.DEFAULT_CHARSET);
    }

}
